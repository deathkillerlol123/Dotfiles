{
    "colors": {
        "dark": {<* for name, value in colors *>
            "{{name}}": "{{value.dark.hex}}"<* if not loop.last *>,<* endif *><* endfor *>
        },
        "light": {<* for name, value in colors *>
            "{{name}}": "{{value.light.hex}}"<* if not loop.last *>,<* endif *><* endfor *>
        }
    }
}